<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Adding New Sequence For A Field-->
    <!--  https://www.youtube.com/watch?v=ZfKzmfiqeg0&list=PLqRRLx0cl0hoJhjFWkFYowveq2Zn55dhM&index=8  -->
    <data noupdate="1">
        <!-- Sequences for hospital.patient -->
        <record id="seq_tender_code" model="ir.sequence">
            <field name="name">Tender code Sequence</field>
            <field name="code">tender.code.sequence</field>
            <field name="prefix">NIT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hospital.appointment -->
        <record id="seq_enquiry_code" model="ir.sequence">
            <field name="name">Enquiry code Sequence</field>
            <field name="code">tender.enquiry.sequence</field>
            <field name="prefix">ENQ</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
